    "babelify": "^7.3.0",
    "budo": "^9.4.7",
    "enzyme": "^2.8.2",
    "jest-cli": "^20.0.4",
    "nodemon": "^1.9.1",
    "react": "^15.0.22",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-test-renderer": "^15.6.1",
